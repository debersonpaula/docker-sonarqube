{
  "executionRoleArn": "arn:aws:iam::664190150621:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "sonar-server",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 9000,
          "protocol": "tcp",
          "containerPort": 9000
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "sonar-ecs"
}
